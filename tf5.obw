; Dimension Variablesdim tf as tabfolderdim ti as longdim ts as stringdim il as imagelistdim NewBMP as bitmapdim OpenBMP as bitmapdim SaveBMP as bitmapdim b1(4) as buttondim l1 as statictextdim sl1 as sliderdim pb as progressbardim t1(4) as textboxdim c1(4) as comboboxdim tab1 as groupdim tab2 as groupdim tab3 as groupdim timer1 as timerOBMain.CREATEcreate NewBMP,OBCNew.bmpcreate OpenBMP,OBCOpen.bmpcreate SaveBMP,OBCSave1.bmpcreate il,NewBMP,OpenBMP,SaveBMPcreate tf,20,20,350,250create b1(1),50,50,80,20 in tab1create b1(2),50,80,80,20 in tab1create b1(3),50,110,80,20 in tab1create b1(4),50,140,80,20 in tab1create l1,200,50,100,20 in tab1create sl1,200,80,100,20 in tab1sl1.setrange 50,200create pb,200,110,100,20 in tab1pb.setrange 50,200pb.backcolor=$ffffpb.forecolor=$80create t1(1),50,50,80,20 in tab2create t1(2),50,80,80,20 in tab2create t1(3),50,110,80,20 in tab2create t1(4),50,140,80,20 in tab2tab2.visible=falsecreate c1(1),50,50,80,20 in tab3create c1(2),50,80,80,20 in tab3create c1(3),50,110,80,20 in tab3create c1(4),50,140,80,20 in tab3tab3.visible=falsetf.SETITEMSIZE 100,18tf.insertitem 1,"Tab1"tf.insertitem 2,"Tab2"tf.insertitem 3,"Tab3"tf.tooltip="ToolTip"tf.image 1,il,2tf.image 2,il,1tf.image 3,ilEND EVENTtf.changeti=tf.selectgosub hideallgroupsselect case ti	case 1		tab1.visible=true	case 2		tab2.visible=true	case 3		tab3.visible=true		l1.group=noneend select	end eventhideallgroups:tab1.visible=falsetab2.visible=falsetab3.visible=falsereturnb1.commandselect case ArrayIndex	case 1		sl1.backcolor=$ff0080	case 2		sl1.backcolor=$ff	case 3		sl1.backcolor=$ffff	case 4		sl1.backcolor=$ff00end select;invoke InvalidateRect,[!sl1]end eventsl1.scrollti=sl1.valuel1.text=str(ti)pb.value=sl1.valueend event