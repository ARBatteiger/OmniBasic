; Dimension Variablesdim dt1 as DATETIMEdim dt2 as DATETIMEdim dt3 as DATETIMEdim l1 as statictextdim l2 as statictextdim l3 as statictextdim x as longdim ts as stringdim ts2 as stringdim ts3 as stringdim timer1 as timerdim ti as longOBMain.CREATEcreate l1,10,10,100,20create l2,10,40,100,20create l3,10,70,100,20ts2="ddd',' MMM dd yy"ts3="hh':'mm':'ss tt"gosub getseedtimer1.time=1000END EVENTtimer1.timeoutdt3=dt1getdatetime dt2x=timediff(dt2,dt1)l1.text=hex(x)timeadd dt3,xGetDateFormat dt3,ts2,tsl2.text=tsGetTimeFormat dt3,ts3,tsl3.text=tsend eventgetseed:dt1.wYear=2002dt1.wMonth=1dt1.wDayOfWeek=0dt1.wDay=1dt1.wHour=0dt1.wMinute=0dt1.wSecond=0dt1.wMilliseconds=0return