; Dimension Variablesdim sb1 as scrollbardim l1 as statictextdim l2 as statictextdim b1 as buttondim tb1 as textboxdim pb1 as progressbardim slider1 as sliderdim sp1 as spinnerdim tl as longdim ts as stringOBMain.CREATEcreate sb1,10,0,300,20create l1,10,40,80,20create l2,100,40,80,20create b1,10,70,80,20create tb1,140,70,80,20tb1.text="0"create pb1,10,100,300,20create slider1,10,130,300,20create sp1,0,0,0,0;create sp1,10,160,20,20sp1.buddy=tb1sp1.setrange 0,100sp1.value=0pb1.backcolor=$ff00pb1.forecolor=$ffffsb1.backcolor=$fftl=sb1.valuel2.text=str(tl)sb1.pagevalue=10slider1.pagevalue=15END EVENTsb1.scrollselect case ScrollCode	case 0		l1.text="LineUp"	case 1		l1.text="LineDn"	case 2		l1.text="PageUp"	case 3		l1.text="PageDn"	case 4		l1.text="ThumbPos"	case 5		l1.text="ThumbTrack"end selecttl=sb1.valuel2.text=str(tl)pb1.value=sb1.valueslider1.value=sb1.valuesp1.value=sb1.valueend eventb1.commandl1.text="force"ts=tb1.textsb1.value=val(ts)tl=sb1.valuel2.text=str(tl)pb1.value=sb1.valueslider1.value=sb1.valuesp1.value=sb1.valueend eventslider1.scrolll1.text="slider"tl=slider1.valuel2.text=str(tl)pb1.value=slider1.valuesb1.value=slider1.valuesp1.value=slider1.valueend eventsp1.scrolll1.text="spinner"tl=sp1.valuesb1.value=tll2.text=str(tl)pb1.value=tlslider1.value=tlend event