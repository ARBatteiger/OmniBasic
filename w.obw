ioButton1,5,30,100,20 IN FrameRadioButton1.TEXT="Red"RadioButton1.FORECOLOR=$0000ffCREATE RadioButton2,5,50,100,20 IN FrameRadioButton2.TEXT="Green"RadioButton2.FORECOLOR=$00ff00CREATE RadioButton3,5,70,100,20 IN FrameRadioButton3.TEXT="Blue"RadioButton3.FORECOLOR=$ff0000CREATE RadioButton4,5,90,100,20 IN FrameRadioButton4.TEXT="Black"RadioButton4.FORECOLOR=$000000CREATE RadioButton5,5,110,100,20 IN FrameRadioButton5.TEXT="White"RadioButton5.FORECOLOR=$ffffffFrame.text="Frame"CREATE t(1),200,10,40,20 in Window4t(1).backcolor=$ffffffCREATE t(2),200,40,40,20 in Window4t(2).backcolor=$ffffffCREATE t(3),200,70,40,20 in Window4t(3).backcolor=$ffffffCREATE t(4),200,100,40,20 in Window4t(4).backcolor=$ffffffCREATE t(5),200,130,40,20 in Window4t(5).backcolor=$ffffffCREATE t(6),250,10,40,20 in Window4t(6).backcolor=$ffffffCREATE t(7),250,40,40,20 in Window4t(7).backcolor=$ffffffCREATE t(8),250,70,40,20 in Window4t(8).backcolor=$ffffffCREATE t(9),250,100,40,20 in Window4t(9).backcolor=$ffffffCREATE t(10),250,130,40,20 in Window4t(10).backcolor=$ffffffCREATE t(11),300,10,40,20 in Window4t(11).backcolor=$ffffffCREATE t(12),300,40,40,20 in Window4t(12).backcolor=$ffffffCREATE t(13),300,70,40,20 in Window4t(13).backcolor=$ffffffCREATE t(14),300,100,40,20 in Window4t(14).backcolor=$ffffffCREATE t(15),300,130,40,20 in Window4t(15).backcolor=$ffffffCREATE t(16),350,10,40,20 in Window4t(16).backcolor=$ffffffCREATE t(17),350,40,40,20 in Window4t(17).backcolor=$ffffffCREATE t(18),350,70,40,20 in Window4t(18).backcolor=$ffffffCREATE t(19),350,100,40,20 in Window4t(19).backcolor=$ffffffCREATE t(20),350,130,40,20 in Window4t(20).backcolor=$fffffffor i=1 to 20	tempint=t(i).handle	tempstr=hex(tempint)	t(i).text=tempstr	;t(i).additem tempstr	;t(i).additem tempstrnext iEND EVENTWindow2.createCREATE b3,10,30,30,20 in Window2b3.text="b3"b2.text="b2"end eventb2.commandte=trueEND EVENTb3.commandb1.text="b1"CREATE Window3