; Dimension Variablesdim x as longdim y(10) as longdim i as longdim a$ as string*32declare fcn uf1(integer,integer) as integerdeclare fcn uf2(string,string) as stringdeclare fcn havis(integer) as integerdeclare proc up1(integer,integer,string)declare proc up2()declare proc up3declare proc up4dim lb as listboxdim b1 as buttonOBMain.CREATEcreate lb,10,10,300,200create b1,10,230,80,20x=uf1(2,7)lb.additem str(x)a$=ucase(uf2("abc","123"))lb.additem a$call proc up1(x+2,3,"String Test")lb.additem str(havis(22))x=havis(x)END EVENTbegin fcn uf1(fx,fy)return fx+fyend fcnbegin fcn uf2(s1$,s2$)return s1$+s2$end fcnbegin proc up1(p1,p2,ps)for i=1 to 3	lb.additem str(p1+p2)	lb.additem psnext iend procbegin fcn havis(h)return 2*hend fcnb1.commandend event