; Dimension VariablesDim LF1 As LOGFONTDIM Font1 AS FONTdim b1 as buttonexdim b2 as buttonexdim b3 as buttonexOBMain.CREATEcreate b1,10,10,50,50LF1.lfHeight=20LF1.lfFaceName="Ravie"CREATE Font1,LF1b1.tooltip="Color Button";b1.enabled=falseb1.backcolor=$80ffb1.text="B1"create b2,90,10,50,50b2.bitmap=1,1b2.style=flatcreate b3,170,10,50,50b3.backcolor=$ffb3.forecolor=$ffffb3.text="Go"b3.font=Font1 END EVENTb1.commandOBMain.text="b1 pressed"end eventb2.commandOBMain.text="b2 pressed"end eventb3.commandOBMain.text="b3 pressed"end eventbegin resourcedirectory RT_BITMAP,bitmaps  resource bitmaps,\           1,LANG_ENGLISH+SUBLANG_DEFAULT,printer,\           2,LANG_ENGLISH+SUBLANG_DEFAULT,cut1  bitmap printer,'OBCPrint.bmp'  bitmap cut1,'OBCCut.bmp'end resource