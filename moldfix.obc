DIM InpFile AS FILEDIM OutFile AS FILEDIM Molds(29155) AS BYTEDIM RecordPtr AS LONGOPEN InpFile,"molds":READGET InpFile,MoldsCLOSE InpFileDELETE "molds"RecordPtr=1Lp:IF Molds(RecordPtr+1)=$20 THEN	Molds(RecordPtr+1)=$00ENDIFIF Molds(RecordPtr+84)=$20 THEN	Molds(RecordPtr+84)=$00ENDIFRecordPtr=RecordPtr+85IF RecordPtr<29155 THEN Lp	CREATE OutFile,"molds":WRITE+BINARYPUT OutFile,MoldsCLOSE OutFile