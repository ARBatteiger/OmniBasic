RestoreTOSX:
IF RSPtr=0 THEN
	StackView.ADDITEM "Return Stack Empty"
ELSE	
	TempStr=RStack(RSPtr)
	StackView.ADDITEM TempStr
	DECR RSPtr
	IF RSPtr<0 THEN
		RSPtr=0
	ENDIF
ENDIF
RETURN

