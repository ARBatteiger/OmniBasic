SELECT CASE StackViewItems
	CASE 0
		StackView.ADDITEM "Filler"
		StackView.ADDITEM "Filler"
		StackView.ADDITEM "Filler"
		StackView.ADDITEM "Filler"
	CASE 1
		StackView.ADDITEM "Filler"
		StackView.ADDITEM "Filler"
		StackView.ADDITEM "Filler"
	CASE 2
		StackView.ADDITEM "Filler"
		StackView.ADDITEM "Filler"
	CASE 3
		StackView.ADDITEM "Filler"
