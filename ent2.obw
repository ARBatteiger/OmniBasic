; Dimension Variablesdim txt1 as textboxdim b1 as buttondim tim1 as timerdim txt2 as textboxdim b2 as buttondim tim2 as timerOBMain.CREATEcreate txt1,10,10,80,20create b1,10,40,80,20create txt2,110,10,80,20create b2,110,40,80,20txt1.interceptontxt2.interceptonEND EVENTb1.commandtxt1.backcolor=$fftim1.interval=300end eventtim1.timertxt1.backcolor=$ffffffend eventtxt1.interceptif wmsg=$102 then	OBMain.text=hex(wparam)	if wparam=$0d then		gosub b1_command	end ifend ifpassinterceptevent	end eventb2.commandtxt2.backcolor=$fftim2.interval=300end eventtim2.timertxt2.backcolor=$ffffffend eventtxt2.interceptif wmsg=$102 then	OBMain.text=hex(wparam)	if wparam=$0d then		gosub b2_command	end ifend ifpassinterceptevent	end event