'********************************'* PROGRAM: lst                 *'* Lists a file                 *'********************************dim InpFile as filedim Buf as string*300Buf=NextArgif len(Buf)=0 then	print "No file specified"	endend ifon error goto BadFileopen Buf for input as InpFileon errorwhile not(eof(InpFile)) do	read InpFile,Buf	print Bufend whileendBadFile:print "Cannot open ";Buf'Modified by new editor