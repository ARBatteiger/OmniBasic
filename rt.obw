; Dimension Variablesdim test1 as filetype rectype	num as long	name as string*6end typedim rec as rectypedim tempint as longdim tempstr as string*6dim go as buttondim numbox as textboxdim namebox as textboxOBMain.CREATEopen "rt" for update as test1rec.num=1rec.name="one"put test1,recrec.num=2rec.name="two"put test1,recrec.num=3rec.name="three"put test1,recrec.num=4rec.name="four"put test1,recrec.num=5rec.name="five"put test1,recrec.num=6rec.name="six"put test1,recrec.num=7rec.name="seven"put test1,recrec.num=8rec.name="eight"put test1,recrec.num=9rec.name="nine"put test1,reccreate go,10,10,80,20create numbox,10,40,80,20create namebox,10,70,80,20end eventgo.commandtempstr=numbox.texttempint=val(tempstr)if tempint<1 or tempint>9 then	namebox.text="Error"	exit eventend ifget test1,rec,tempintnumbox.text=str(rec.num)namebox.text=rec.nameend event