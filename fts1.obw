; Dimension Variablesdim test1 as filedim x(9,9) as bytedim row as longdim col as longdim b1 as buttondim t1 as textboxdim t3 as textboxdim t4 as textboxdim tempint as longdim tempstr as stringOBMain.CREATEopen "vb1" for input as test1get test1,xclose test1create t1,10,10,90,20create t3,10,40,40,20create t4,60,40,40,20create b1,10,70,90,20END EVENTb1.commandtempstr=t3.textrow=val(tempstr)tempstr=t4.textcol=val(tempstr)tempint=x(row,col)t1.text=hex(tempint)end event