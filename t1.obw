; Dimension Variablesdim timer1 as timerdim timer2(10) as timerdim timer3 as timerdim b1 as buttondim b2 as buttondim b3 as buttondim l1 as statictextOBMain.CREATEconstruct b1,10,10,80,20b1.text="timer2(3)"construct b2,100,10,80,20b2.text="timer2(4)"construct b3,190,10,80,20b3.text="timer1"construct l1,10,40,80,20l1.backcolor=$ffffEND EVENTb1.commandl1.text=""timer2(3).interval=1000end eventb2.commandl1.text=""timer2(4).interval=5000end eventb3.commandl1.text=""timer1.interval=2000end eventtimer1.timerl1.text=str(ArrayIndex)timer1.stopend eventtimer2.timerl1.text=str(ArrayIndex)timer2(ArrayIndex).stopend eventtimer3.timertimer3.stopend event