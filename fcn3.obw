; Dimension Variablesdim x as longdim a$ as string*32dim s as string*32dim y(10) as bytedeclare fcn uf1(integer,integer) as integerdeclare fcn uf2(string,string) as stringdeclare fcn q(integer) as integerdeclare proc up1(integer,integer,string)declare proc up2()declare proc up3declare proc up4dim lb as listboxdim b1 as buttonOBMain.CREATEcreate lb,10,10,300,200create b1,10,230,80,20y(3)=7s="abc"x=uf1(2,y(3))lb.additem str(x)a$=ucase(uf2(s,"123"))lb.additem a$call proc up1(x+2,3,"String Test")lb.additem str(q(22))x=q(x)END EVENTbegin fcn uf1(a,b)return a+bend fcnbegin fcn uf2(a,b)return a+bend fcnbegin proc up1(a,b,c)lb.additem str(a+b)lb.additem cend procbegin fcn q(a)return 2*aend fcnb1.commandend event