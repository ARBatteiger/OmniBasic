; Dimension Variablesdim icon1 as icondim icon2 as icondim icon3 as icondim b1 as iconbuttondim b2 as iconbuttondim b3 as iconbuttondim sb as statusbardim sbParts(3) as longOBMain.CREATEcreate sb,0,0,0,0sbParts(1)=100sbParts(2)=200sbParts(3)=300sb.SETPARTS 3,sbPartscreate icon1,house.icocreate icon2,house.ico,smallcreate icon3,house.ico,smallcreate b1,10,10,36,36create b2,50,10,20,20create b3,100,10,36,36b1.icon=icon1b2.icon=icon2b3.icon=icon3sb.seticon 1,icon1sb.seticon 2,icon2END EVENT