; Dimension Variablesdim tf as tabfolderdim b1 as buttondim b2 as buttondim b3 as buttondim b4 as buttondim t1 as textboxdim t2 as textboxdim t3 as textboxdim l1 as statictextdim ti as longdim ts as stringdim il as imagelistdim NewBMP as bitmapdim OpenBMP as bitmapdim SaveBMP as bitmapOBMain.CREATEcreate NewBMP,OBCNew.bmpcreate OpenBMP,OBCOpen.bmpcreate SaveBMP,OBCSave1.bmpcreate il,NewBMP,OpenBMP,SaveBMPcreate tf,20,20,350,100tf.SETITEMSIZE 100,18create b1,20,140,30,20b1.text="1"create b2,60,140,30,20b2.text="2"create b3,100,140,30,20b3.text="3"create b4,140,140,30,20create l1,200,200,100,20create t1,20,170,30,20create t2,60,170,30,20create t3,100,170,30,20l1.backcolor=$ffffl1.text="Test"tf.insertitem 1,"Tab1"tf.insertitem 2,"Tab2"tf.insertitem 3,"Tab3"tf.tooltip="ToolTip"tf.image 1,iltf.image 2,iltf.image 3,ilEND EVENTtf.changeti=tf.selectl1.text=str(ti)l1.backcolor=$ffend eventt1.changets=t1.texttf.text 1,tsEND EVENTt2.changets=t2.texttf.text 2,tsEND EVENTt3.changets=t3.texttf.text 3,tsEND EVENTb1.commandtf.select=1gosub dsplyEND EVENTb2.commandtf.select=2gosub dsplyEND EVENTb3.commandtf.select=3gosub dsplyEND EVENTdsply:ti=tf.selectts=str(ti)l1.text=tsRETURNb4.commandtf.deleteitem 2l1.visible=falsel1.visible=trueend event